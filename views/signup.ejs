<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <script src="http://code.jquery.com/jquery-3.3.1.min.js"
  integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
  crossorigin="anonymous"></script>
  <link href="https://fonts.googleapis.com/css?family=Raleway|Gaegu|Roboto+Slab|Merienda|Roboto|Merienda|Acme|Righteous|Khand" rel="stylesheet">
    <link rel="stylesheet" href="/assets/style.css" type="text/css">
    <title>Marinara Organic</title>
  </head>
  <body>
    <div class="container">


        <div class="gTitle">Create New Account</div>

        <!-- login form -->
        <form id="signup" name="signup" method="post" action="/signup">
          <div class="form-group">
            <label class="inpLbl" for="firstname">Firstname</label>
            <input name="firstname" type="text" />
          </div>
          <div class="form-group">
            <label class="inpLbl" for="lastname">Lastname</label>
            <input name="lastname" type="text" />
          </div>
          <div class="form-group">
            <label class="inpLbl" for="username">Username</label>
            <input name="username" type="text" />
          </div>
          <div class="form-group">
            <label class="inpLbl" for="email">Email</label>
            <input name="email" type="email">
          </div>
          <div class="form-group">
            <label class="inpLbl" for="password">Password</label>
            <input type="password" name="password">
          </div>
          <button type="submit" class="subBtnSu">Sign Up</button>
        </form>

        <hr>

        <p class = "gmo">Already have account? <a href="/signin">Log In</a></p>
        <p class="nApp-su"><a href="/todo">Skip</a></p>

      </div>
    </div>
    <script src = "/assets/script.js" type="text/javascript"></script>
  </body>
</html>
